<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.numberone.project.gistools.twoDStyle.mapper.StyleMapper">
    <insert id="insertPicture">
        insert gt_picture (id,base,name) values (#{id},#{base},#{name})
    </insert>

    <select id="findWorkSpace" resultType="java.util.Map">
        SELECT t.* FROM bus_publish_workspace t
        WHERE 1=1
    </select>
</mapper>
